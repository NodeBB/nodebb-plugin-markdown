  },
  "nbbpm": {
    "compatibility": "^1.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-angular": "^12.1.4",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.3",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4"
  }
}
