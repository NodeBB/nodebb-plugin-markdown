  "dependencies": {
    "async": "^2.6.0",
    "highlight.js": "9.14.2",
    "markdown-it": "^8.4.2",
    "markdown-it-checkbox": "^1.1.0"
  },
  "nbbpm": {
    "compatibility": "^1.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.0",
    "@commitlint/config-angular": "^7.5.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3"
  }
}
